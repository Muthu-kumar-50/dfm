<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="Python" path="*.py" verb="*" modules="CgiModule" scriptProcessor="C:\Users\Administrator\AppData\Local\Programs\Python\Python37\python.exe %%" resourceType="File" />
        </handlers>
    </system.webServer>
</configuration>
